<!-- BEGIN forumrow -->
	<!-- IF forumrow.S_IS_CAT -->
		<category>
      <id>{forumrow.FORUM_ID}</id>
      <name>{forumrow.FORUM_NAME}</name>
		</category>
	<!-- ELSEIF forumrow.S_IS_LINK -->
    <link>
      <id>{forumrow.FORUM_ID}</id>
      <name>{forumrow.FORUM_NAME}</name>
		</link>
	<!-- ELSE -->
    <subforum>
      <id>{forumrow.FORUM_ID}</id>
      <name>{forumrow.FORUM_NAME}</name>
    </subforum>
  <!-- ENDIF -->
<!-- END forumrow -->